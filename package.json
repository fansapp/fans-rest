    "babel-loader": "7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.2",
    "es6-promise": "4.1.0",
    "eslint": "4.0.0",
    "isomorphic-fetch": "2.2.1",
